{
  "name": "gpt-pdf",
  "version": "0.0.1",
  "description": "",
  "main": "build/cli.js",
  "bin": {
    "gpt-pdf": "build/cli.js"
  },
  "pkg": {
    "scripts": "./build/**/*.js",
    "targets": [
      "node16-win-x64"
    ],
    "assets": [
      "./node_modules/canvas/build/Release/*",
      "./node_modules/pdfjs-dist/build/*",
      "./node_modules/pdfjs-dist/legacy/build/*"
    ],
    "outputPath": "dist"
  },
  "scripts": {
    "package": "pkg .",
    "dev": "tsx ./src/cli.ts",
    "build": "tsup src --out-dir build",
    "start": "node build/cli.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.15.9",
    "@types/yargs": "^17.0.23",
    "pkg": "^5.8.1",
    "tsup": "^6.7.0",
    "tsx": "^3.12.6",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "handlebars": "^4.7.7",
    "openai": "^3.2.1",
    "pdfjs-dist": "^3.4.120",
    "yargs": "^17.7.1"
  }
}